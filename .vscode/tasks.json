{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "hxml",
			"file": "compile.hxml",
			"problemMatcher": [
				"$haxe-absolute",
				"$haxe",
				"$haxe-error",
				"$haxe-trace"
			],
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"label": "haxe: compile.hxml"
		},
		{
			"type": "hxml",
			"file": "debug_compile.hxml",
			"problemMatcher": [
				"$haxe-absolute",
				"$haxe",
				"$haxe-error",
				"$haxe-trace"
			],
			"group": "build",
			"label": "haxe: debug compile.hxml"
		},
		{
			"type": "haxe",
			"args": "active configuration",
			"problemMatcher": [
				"$haxe-absolute",
				"$haxe",
				"$haxe-error",
				"$haxe-trace"
			],
			"group": "build",
			"label": "haxe: active configuration"
		}
	]
}